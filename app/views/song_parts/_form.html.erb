<%= form_for(@song_part) do |f| %>
  <% if @song_part.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@song_part.errors.count, "error") %> prohibited this song_part from being saved:</h2>

      <ul>
      <% @song_part.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :song_id %><br>
    <%= f.select :song_id, Song.all.collect { |s| [ s.name, s.id] }, { include_blank: true } %>
  </div>
  <div class="field">
    <%= f.label :song_template_id %><br>
    <%= f.select :song_template_id, SongTemplate.all.collect { |st| [ st.name, st.id] }, { include_blank: true } %>
  </div>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :scanned %><br>
    <%= f.check_box :scanned %>
  </div>
  <div class="field">
    <%= f.label :notes %><br>
    <%= f.text_area :notes %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<fieldset>
  <legend>Parts:</legend>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Notes</th>
        <th>Instruments</th>
        <th>Scanned</th>
        <th>Order</th>
        <th>Remove</th>
      </tr>
    </thead>
    <tbody>
      <%= f.fields_for :song_parts do |parts_form| %>
        <%= render 'shared/song_part_fields', f: parts_form %>
      <% end %>
    </tbody>
  </table>
  <%= link_to_add_fields "Add Part", f, :song_parts, 'shared' %>
</fieldset>
